  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run clean && tsc -p ./ && npm run copy-content && npm run clean-pycache",
    "clean": "rm -rf dist/",
    "clean-pycache": "find dist -type d -name '__pycache__' -exec rm -rf {} + 2>/dev/null || true",
    "copy-content": "mkdir -p dist/content dist/src/webview dist/src/telemetry dist/assets/img dist/themes && cp -r content/lessons dist/content/ && cp -r content/templates dist/content/ && cp -r content/pages dist/content/ && cp -r content/projects dist/content/ && cp content/lesson-registry.json dist/content/ && cp -r src/webview/* dist/src/webview/ && cp src/telemetry/*.py dist/src/telemetry/ && cp -r assets/img/* dist/assets/img/ && cp -r themes/* dist/themes/",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "compile": "tsc -p ./",
    "test": "mocha",
    "test:templates": "mocha test/lesson-tests/templates.test.ts",
    "test:watch": "mocha --watch"
  },
