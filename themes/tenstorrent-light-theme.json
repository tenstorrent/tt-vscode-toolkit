  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6c6960",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#3293b2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords",
      "scope": [
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#5347a4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#3293b2"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#2b6c82"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#82672b"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#5347a4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#5347a4"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#d03a1b"
      }
    },
    {
      "name": "String Interpolation",
      "scope": [
        "string.template meta.template.expression",
        "string.template punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#2b6c82"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#ca982b"
      }
    },
    {
      "name": "Built-in Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#5347a4"
      }
    },
    {
      "name": "User-defined Constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#82672b"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#2b6c82"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter",
        "meta.parameters"
      ],
      "settings": {
        "foreground": "#3293b2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#3293b2"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#82672b"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#4c5761"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#3293b2"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#3293b2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#82672b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#d03a1b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#2b6c82"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#3293b2",
        "fontStyle": "underline"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#5347a4"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#82672b"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#d03a1b",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#82672b",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
