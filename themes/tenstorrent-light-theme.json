    "gitDecoration.untrackedResourceForeground": "#82672b",
    "gitDecoration.ignoredResourceForeground": "#98a3ae",
    "gitDecoration.conflictingResourceForeground": "#5347a4",
    "gitDecoration.submoduleResourceForeground": "#3fb7de",

    // === Settings Editor ===
    "settings.headerForeground": "#3293b2",
    "settings.modifiedItemIndicator": "#3293b2",
    "settings.dropdownBackground": "#FFFFFF",
    "settings.dropdownForeground": "#2b3036",
    "settings.dropdownBorder": "#98a3ae",
    "settings.checkboxBackground": "#FFFFFF",
    "settings.checkboxForeground": "#2b3036",
    "settings.checkboxBorder": "#98a3ae",
    "settings.textInputBackground": "#FFFFFF",
    "settings.textInputForeground": "#2b3036",
    "settings.textInputBorder": "#98a3ae",
    "settings.numberInputBackground": "#FFFFFF",
    "settings.numberInputForeground": "#2b3036",
    "settings.numberInputBorder": "#98a3ae",

    // === Breadcrumbs ===
    "breadcrumb.foreground": "#4c5761",
    "breadcrumb.background": "#FFFFFF",
    "breadcrumb.focusForeground": "#3293b2",
    "breadcrumb.activeSelectionForeground": "#3293b2",
    "breadcrumbPicker.background": "#FFFFFF",

    // === Snippets ===
    "editor.snippetTabstopHighlightBackground": "#c6edfa",
    "editor.snippetTabstopHighlightBorder": "#3293b2",
    "editor.snippetFinalTabstopHighlightBackground": "#efedfd",
    "editor.snippetFinalTabstopHighlightBorder": "#5347a4",

    // === Symbol Icons ===
    "symbolIcon.arrayForeground": "#82672b",
    "symbolIcon.booleanForeground": "#5347a4",
    "symbolIcon.classForeground": "#5347a4",
    "symbolIcon.colorForeground": "#d03a1b",
    "symbolIcon.constantForeground": "#ca982b",
    "symbolIcon.constructorForeground": "#82672b",
    "symbolIcon.enumeratorForeground": "#5347a4",
    "symbolIcon.enumeratorMemberForeground": "#2b6c82",
    "symbolIcon.eventForeground": "#5347a4",
    "symbolIcon.fieldForeground": "#2b6c82",
    "symbolIcon.fileForeground": "#2b3036",
    "symbolIcon.folderForeground": "#3293b2",
    "symbolIcon.functionForeground": "#82672b",
    "symbolIcon.interfaceForeground": "#5347a4",
    "symbolIcon.keyForeground": "#3293b2",
    "symbolIcon.keywordForeground": "#3293b2",
    "symbolIcon.methodForeground": "#82672b",
    "symbolIcon.moduleForeground": "#2b6c82",
    "symbolIcon.namespaceForeground": "#5347a4",
