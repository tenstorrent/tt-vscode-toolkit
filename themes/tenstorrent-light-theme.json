    "terminal.ansiCyan": "#2b6c82",
    "terminal.ansiGreen": "#499c8d",
    "terminal.ansiMagenta": "#5347a4",
    "terminal.ansiRed": "#d03a1b",
    "terminal.ansiWhite": "#e9edf1",
    "terminal.ansiYellow": "#ca982b",
    "terminal.selectionBackground": "#c6edfa",
    "terminalCursor.background": "#FFFFFF",
    "terminalCursor.foreground": "#3293b2",

    // === Debug ===
