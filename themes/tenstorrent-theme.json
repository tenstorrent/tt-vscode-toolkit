  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#607D8B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#4FD1C5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords",
      "scope": [
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#EC96B8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#4FD1C5"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#81E6D9"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#F4C471"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#EC96B8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#EC96B8"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#F5A7C0"
      }
    },
    {
      "name": "String Interpolation",
      "scope": [
        "string.template meta.template.expression",
        "string.template punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#81E6D9"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#E6B55E"
      }
    },
    {
      "name": "Built-in Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#EC96B8"
      }
    },
    {
      "name": "User-defined Constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#F4C471"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#81E6D9"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter",
        "meta.parameters"
      ],
      "settings": {
        "foreground": "#B8E6D9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#4FD1C5"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#F4C471"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#B0C4DE"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#4FD1C5"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#4FD1C5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#F4C471",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#F5A7C0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#81E6D9"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#4FD1C5",
        "fontStyle": "underline"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#EC96B8"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#F4C471"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF6B6B",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#F4C471",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
